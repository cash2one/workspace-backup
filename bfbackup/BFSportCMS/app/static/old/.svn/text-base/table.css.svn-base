/* CSS Document */
table{
	font-family:Arial, Helvetica, sans-serif;
	width:100%;
	border-collapse:collapse
}
th{
	font-size:12px;
  	border:1px solid #0066ff;
	padding:3px 2px 2px 2px;
}
.thead th{
	font-size:12px;
	border:1px solid #0066ff;
	padding:2px 0px 1px 0px;
	text-align:center;
 	color:#00008B;
}
.tbody th{
	font-size:12px;
  	border:1px solid #0066ff;
	padding:3px 2px 2px 2px;
}
thead th{
	font-size:12px;
	border:1px solid #0066ff;
	padding:2px 0px 1px 0px;
	text-align:center;
 	color:#00008B;
}
tbody th{
	font-size:12px;
  	border:1px solid #0066ff;
	padding:3px 0px 0px 0px;
}
